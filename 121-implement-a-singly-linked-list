public class LinkedList{
    private Node head;
    private Node tail;
    private Integer size = 0;
    
    public void addToFront(Integer value){
        Node newNode = new Node(value);

        if(head == null){
            head = newNode;
            tail = newNode;
        }
        else {
            newNode.next = head;
            head = newNode;
        }

        size++;
    }

    public Integer removeFromFront(){
        if(head == null){
            return null;
        }
        
        Integer removed = head.data;
        head = head.next;

        if(head == null){
            tail = null;
        }

        return removed;
    }
    public void addToTail(Integer value){}
    public Integer removeFromTail(){}
    public Integer size(){}
}

public class Node{
    Integer data;
    Node next;

    public Node(Integer data){
        this.data = data;
        this.next = null;
    }
}