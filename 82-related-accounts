public Set<Id> accountIds(List<Opportunity> opps) {
    //code here
    Set<Id> extractedIds = new Set<Id>();
    if(!opp.isEmpty()){
        for(Integer i = opps.size() - 1; i > 0; i--){
            if(opps[i].AccountId == opps[i - 1].AccountId){
                extractedIds.add(opps[i].AccountId);
            }
        }
    }

    return extractedIds;
}