public Map<String, List<Case>> casesByType(List<Case> cases) {
    //code here
    Map<String, List<Case>> orderedCases = new Map<String, List<Case>>();

    for(Case case : cases){
        if(!orderedCases.containsKey(case.Type)){
            orderedCases.put(case.Type, null);
        }

        
    }

    return orderedCases;
}