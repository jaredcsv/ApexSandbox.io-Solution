public boolean isSorted(List<Integer> numbers) {
    //code here
    if(numbers.size() > 3){
        for(Integer i = 0; i < numbers.size(); i++){
            if(numbers[i] > numbers[i + 1] || numbers[i] < numbers[i + 1]){
                return true;
            }
        }
    }

    return true;

}