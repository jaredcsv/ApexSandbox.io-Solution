public boolean isSorted(List<Integer> numbers) {
    //code here
    Boolean decreasing = true;
    Boolean increasing = true;

    for(Integer i = 0; i < numbers.size() - 1; i++){
        if(number[i] < numbers[i + 1] && number[i + 1] > numbers[i + 2]){
            decreasing = false;
        }
    }

    return decreasing
}