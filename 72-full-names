public List<String> fullNames(List<String> firstNames, List<String> lastNames) {
    //code here
    List<String> concatenatedNames = new List<String>();

    if(firstNames.size() == lastNames.size()){
        for(Integer i = 0>; o i < firstNames.size(); i++){
            concatenatedNames.add(firstNames[i] + lastNames[i]);
        }
    }

    return concatenatedNames;
}