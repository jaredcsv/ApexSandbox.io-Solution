public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    Integer onecounter = 0;
    List<Integer> onerows = new List<Integer>();

    for(Integer i = 0; i < numbers.size(); i++) {
        if(numbers[i] == 1) {
            onecounter++;
        } else {
            if (onecounter > 0) {
                onerows.add(onecounter);
                onecounter = 0; // Reiniciar el contador
            }
        }
    }

        // Agregar el último contador si la secuencia de 1 termina al final de la matriz
    if (onecounter > 0) {
        onerows.add(onecounter);
    }

    Integer maxCount = 0;
    
    for(Integer count : onerows) {
        maxCount = Math.max(maxCount, count);
    }

    return maxCount;
}