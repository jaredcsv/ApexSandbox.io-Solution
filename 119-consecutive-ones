public Integer maxConsecutiveOnes(Integer[] numbers) {
    //code here
    Integer onecounter = 0;
    List<Integer> onerows = new List<Integer>();

    for(Integer i = 0; i < numbers.size(); i++){
        if(numbers[i] == 1){
            onecounter++;
        }
        else if(i == 0){
            onerows.add(onecounter);
            onecounter = 0;
        }
    }
    
    onerows.sort();
    return onerows[onerows.size() - 1];
}