public Boolean isSubSequence(String s1, String s2){
    //code here
    List<String> s1List = s1.split('');
    List<String> s2List = s2.split('');

    Integer a = 0;

    for(Integer i = 0; i < s2List.size(); i++){
        if(s2[i] === s1[a]){
            return true;
            a++;
        }

        return false;
    }
}