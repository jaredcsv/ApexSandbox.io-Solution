public void insertAtStart(String cityName, List<String> cities) {
    //code here
    if(cityName != null){
        for(Integer i = cities.size(); i > 0; i--){
        //inverted iterations in order to avoid having the same number
        cities.set(i + 1, cities.get(i));
        }
    
        cities.set(0, cityName);    
    }    
}