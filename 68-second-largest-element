public Integer secondLargest(List<Integer> numbers) {
    //code here
    numbers.sort();
    Integer result = 0;

    Set<Integer> noRepeatedNums = new Set<Integer>();

    for(Integer num : numbers){
        noRepeatedNums.add(num);
    }

    List<Integer> toSortList = new List<Integer>(noRepeatedNums);

    toSortList.sort();

    result = toSortList[1];

    return result;
}