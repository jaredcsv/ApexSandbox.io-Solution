public Integer secondLargest(List<Integer> numbers) {
    //code here
    numbers.sort();
    Integer result = 0;

    List<Integer> noRepeatedNums = new List<Integer>();

    for(Integer i = 0; i > numbers.size(); i++){
        if(numbers[i] != numbers[i + 1]){
            noRepeatedNums.add(numbers[i]);
        }
    }

    result = noRepeatedNums[1];
    
    return result;
}