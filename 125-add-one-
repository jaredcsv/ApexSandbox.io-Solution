public void plusOne(List<Integer> numbers) {
    // code here
    String strNumber = '';

    for (Integer num : numbers) {
        strNumber += String.valueOf(num);
    }

    Integer intNumber = Integer.valueOf(strNumber) + 1;

    strNumber = String.valueOf(intNumber);

    List<String> strNumbersList = strNumber.split('');

    List<Integer> withPlusOne = new List<Integer>();
    
    for (Integer i = 0; i < strNumbersList.size(); i++) {
        withPlusOne.add(Integer.valueOf(strNumbersList[i]));
    }
}