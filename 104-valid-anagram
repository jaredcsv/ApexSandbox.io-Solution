public boolean isAnagram(String s1, String s2) {
    //Code here

    List<String> s1List = s1.split('');
    List<String> s2List = s2.split('');
    
    s1List.sort();
    s2List.sort();

    for(Integer i = 0; i < s1List.size(); i++){
        if(s1List[i] == s2List[i]){
            return true;
        }
        return false;
    }
}